<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.com.ingatech.trabalho">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".listas.ListaCidadesActivity" />
        <activity android:name=".cad.CadCidadeActivity"></activity>
        <activity android:name=".listas.ListaClienteActivity" />
        <activity android:name=".cad.CadClienteActivity"></activity>
        <activity android:name=".listas.ListaFornecedoresActivity" />
        <activity android:name=".cad.CadFornecedoresActivity"></activity>

    </application>

</manifest>